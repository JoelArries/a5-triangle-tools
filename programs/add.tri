let 	
 var a: Integer;
 var b: Integer;
 var added: Integer;
 var multiplied: Integer
 
in
 begin
 getint (var a);
 getint (var b);
 added := a + b;
 multiplied := a * b;
 if added = multiplied then
  begin
  put ('S'); put ('a'); put ('m'); put ('e'); put ('!'); end
 else
  begin
  putint (added); put (' '); putint (multiplied); end;
 end
